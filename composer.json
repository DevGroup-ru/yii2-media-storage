{
    "name": "devgroup/yii2-media-storage",
    "description": "Helpers, utilities and widgets for media data",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Ivan Salnikov",
            "email": "salnikov@devgroup.ru"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "^2.0.6",
        "yiisoft/yii2-imagine": "*",
        "branchonline/yii2-lightbox": "*",
        "devgroup/yii2-multilingual": "dev-master",
        "devgroup/yii2-users-module": "dev-master",
        "devgroup/yii2-dropzone": "^1.0",
        "mihaildev/yii2-elfinder": "^1.1.3",
        "2amigos/yii2-transliterator-helper": "^0.1.0",
        "mihaildev/yii2-elfinder-flysystem": "^0.0.1",
        "devgroup/yii2-data-structure-tools": "*",
        "devgroup/yii2-tag-dependency-helper": "*",
        "rmrevin/yii2-fontawesome": "~2.14",
        "league/glide": "^1.0"
    },
    "require-dev": {
        "yiisoft/yii2-bootstrap": "^2.0@dev",
        "devgroup/yii2-admin-utils": "dev-master",
        "devgroup/dotplant-dev": "~1.0.0"
    },
    "autoload": {
        "psr-4": {
            "DevGroup\\MediaStorage\\": "src/"
        },
        "classmap" : ["Filesystem.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "DevGroup\\MediaStorage\\Tests\\": "tests/"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }

    }
}
